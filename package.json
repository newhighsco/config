{
  "name": "@newhighsco/postcss-config",
  "description": "New High Score shareable config for PostCSS",
  "version": "1.0.3",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/postcss-config.git"
  },
  "homepage": "https://github.com/newhighsco/postcss-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/postcss-config/issues"
  },
  "main": "postcss.config.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "autoprefixer": "9.8.6",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "6.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@newhighsco/commitlint-config": "1.0.5",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.18",
    "@newhighsco/prettier-config": "2.0.3",
    "@newhighsco/release-config": "1.0.26",
    "eslint": "7.7.0",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  }
}
