{
  "name": "@newhighsco/eslint-config",
  "description": "New High Score shareable config for ESLint",
  "version": "2.2.24",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/eslint-config.git"
  },
  "homepage": "https://github.com/newhighsco/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/eslint-config/issues"
  },
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json .",
    "lint:jsx": "npm run lint:js -- --ext .js,.json,.jsx,.mdx --config ./react.js",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix",
    "format:jsx": "npm run lint:jsx -- --fix"
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-mdx": "1.8.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-standard": "4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@newhighsco/commitlint-config": "1.0.6",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/prettier-config": "2.0.5",
    "@newhighsco/release-config": "1.0.28",
    "eslint": "7.8.1",
    "husky": "4.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1"
  },
  "peerDependencies": {
    "eslint": "7.8.1",
    "prettier": "2.1.1"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": "."
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "packageRules": [
      {
        "extends": [
          "packages:eslint"
        ],
        "packageNames": [
          "prettier"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
