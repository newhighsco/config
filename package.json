{
  "name": "@newhighsco/postcss-config",
  "description": "New High Score shareable config for PostCSS",
  "version": "3.0.101",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/postcss-config.git"
  },
  "homepage": "https://github.com/newhighsco/postcss-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/postcss-config/issues"
  },
  "main": "index.js",
  "files": [
    "purgecss.js"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "4.0.3",
    "autoprefixer": "10.2.5",
    "caniuse-lite": "1.0.30001228",
    "postcss": "8.3.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "6.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@newhighsco/commitlint-config": "1.0.12",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.91",
    "@newhighsco/prettier-config": "2.0.9",
    "@newhighsco/release-config": "1.0.60",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "semantic-release": "17.4.3"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  }
}
