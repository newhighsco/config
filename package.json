{
  "name": "@newhighsco/eslint-config",
  "description": "New High Score shareable config for ESLint",
  "version": "2.3.46",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/eslint-config.git"
  },
  "homepage": "https://github.com/newhighsco/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/eslint-config/issues"
  },
  "main": "index.js",
  "scripts": {
    "test": "yarn lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.json .",
    "lint:jsx": "yarn lint:js --ext .js,.json,.jsx,.mdx --config ./react.js",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix",
    "format:jsx": "yarn lint:jsx --fix"
  },
  "files": [
    "react.js"
  ],
  "dependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mdx": "1.16.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-standard": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@newhighsco/commitlint-config": "1.0.18",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/prettier-config": "2.0.15",
    "@newhighsco/release-config": "1.1.4",
    "eslint": "8.3.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.0",
    "semantic-release": "18.0.1"
  },
  "peerDependencies": {
    "eslint": "8.3.0",
    "prettier": "2.5.0"
  },
  "resolutions": {
    "trim": "^1.0.0"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": "."
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "packageRules": [
      {
        "extends": [
          "packages:eslint"
        ],
        "baseBranches": [
          "main"
        ],
        "packageNames": [
          "prettier"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
