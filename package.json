{
  "name": "@newhighsco/eslint-config",
  "description": "New High Score shareable config for ESLint",
  "version": "1.1.1",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/eslint-config.git"
  },
  "homepage": "https://github.com/newhighsco/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/eslint-config/issues"
  },
  "engines": {
    "node": "^13.7.0",
    "yarn": "^1.19.1"
  },
  "main": "eslint.config.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "eslint-config-prettier": "6.10.0",
    "eslint-config-prettier-standard": "3.0.1",
    "eslint-config-standard": "14.1.0",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-standard": "4.0.1",
    "prettier-config-standard": "1.0.1"
  },
  "devDependencies": {
    "@newhighsco/prettier-config": "1.0.2",
    "@newhighsco/release-config": "1.0.14",
    "@newhighsco/renovate-config": "1.0.0",
    "eslint": "6.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "semantic-release": "17.0.4"
  },
  "peerDependencies": {
    "eslint": "6.8.0",
    "prettier": "1.19.1"
  },
  "prettier": "@newhighsco/prettier-config",
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  }
}
