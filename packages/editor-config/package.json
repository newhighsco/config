{
  "name": "@newhighsco/editor-config",
  "description": "New High Score shareable config for EditorConfig",
  "version": "1.1.1",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/editor-config.git"
  },
  "homepage": "https://github.com/newhighsco/editor-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/editor-config/issues"
  },
  "main": "index.js",
  "files": [
    ".editorconfig"
  ],
  "scripts": {
    "postinstall": "node ./index.js",
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@newhighsco/commitlint-config": "1.0.8",
    "@newhighsco/eslint-config": "2.2.67",
    "@newhighsco/prettier-config": "2.0.8",
    "@newhighsco/release-config": "1.0.51",
    "eslint": "7.20.0",
    "husky": "5.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "semantic-release": "17.4.0"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  }
}
